    <color name="redPressedB60E34">#b60e34</color>
    <color name="redDisable666666">#666666</color>
    <color name="orangePressedFFAF25">#ffaf25</color>
    <color name="orangeDisable999999">#999999</color>
    <color name="whitePressedF0F0F0">#f0f0f0</color>
