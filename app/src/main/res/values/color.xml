    <color name="redE11644">#e11644</color>
    <color name="yellowFFAF25">#ffaf25</color>
    <color name="pinkFC8080">#fc8080</color>
    <color name="blue4BA0E9">#4ba0e9</color>
    <color name="purple8B8CD5">#8b8cd5</color>
    <color name="hotpinkFF70a8">#ff70a8</color>
